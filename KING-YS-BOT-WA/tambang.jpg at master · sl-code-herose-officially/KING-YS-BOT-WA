.details-reset > summary:focus-visible { outline: 2px solid var(--color-accent-fg); outline-offset: -2px; box-shadow: none; }

.details-reset > summary.btn-primary:focus { outline: 2px solid var(--color-accent-fg); outline-offset: -2px; box-shadow: inset 0 0 0 3px var(--color-fg-on-emphasis); }

.details-reset > summary.btn-primary:focus:not(:focus-visible) { outline: transparent solid 1px; box-shadow: none; }

.details-reset > summary.btn-primary:focus-visible { outline: 2px solid var(--color-accent-fg); outline-offset: -2px; box-shadow: inset 0 0 0 3px var(--color-fg-on-emphasis); }

.details-reset > summary::before { display: none; }

.details-reset > summary::-webkit-details-marker { display: none; }

